# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
source/GhostGame/.utmp/
source/GhostGame/[Ll]ibrary/
source/GhostGame/[Tt]emp/
source/GhostGame/[Oo]bj/
source/GhostGame/[Bb]uild/
source/GhostGame/[Bb]uilds/
source/GhostGame/[Ll]ogs/
source/GhostGame/[Uu]ser[Ss]ettings/
source/GhostGame/*.log

# By default unity supports Blender asset imports, *.blend1 blender files do not need to be commited to version control.
source/GhostGame/*.blend1
source/GhostGame/*.blend1.meta

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
source/GhostGame/[Mm]emoryCaptures/

# Recordings can get excessive in size
source/GhostGame/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
source/GhostGame/[Aa]ssets/Plugins/Editor/JetBrains*
# Jetbrains Rider personal-layer settings
source/GhostGame/*.DotSettings.user

# Visual Studio cache directory
source/GhostGame/.vs/

# Gradle cache directory
source/GhostGame/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
source/GhostGame/ExportedObj/
source/GhostGame/.consulo/
source/GhostGame/*.csproj
source/GhostGame/*.unityproj
source/GhostGame/*.sln
source/GhostGame/*.suo
source/GhostGame/*.tmp
source/GhostGame/*.user
source/GhostGame/*.userprefs
source/GhostGame/*.pidb
source/GhostGame/*.booproj
source/GhostGame/*.svd
source/GhostGame/*.pdb
source/GhostGame/*.mdb
source/GhostGame/*.opendb
source/GhostGame/*.VC.db

# Unity3D generated meta files
source/GhostGame/*.pidb.meta
source/GhostGame/*.pdb.meta
source/GhostGame/*.mdb.meta

# Unity3D generated file on crash reports
source/GhostGame/sysinfo.txt

# Mono auto generated files
source/GhostGame/mono_crash.*

# Builds
source/GhostGame/*.apk
source/GhostGame/*.aab
source/GhostGame/*.unitypackage
source/GhostGame/*.unitypackage.meta
source/GhostGame/*.app

# Crashlytics generated file
source/GhostGame/crashlytics-build.properties

# TestRunner generated files
source/GhostGame/InitTestScene*.unity*

# Addressables default ignores, before user customizations
source/GhostGame/ServerData
source/GhostGame/[Aa]ssets/StreamingAssets/aa*
source/GhostGame/[Aa]ssets/AddressableAssetsData/link.xml*
source/GhostGame/[Aa]ssets/Addressables_Temp*
# By default, Addressables content builds will generate addressables_content_state.bin
# files in platform-specific subfolders, for example:
# /Assets/AddressableAssetsData/OSX/addressables_content_state.bin
source/GhostGame/[Aa]ssets/AddressableAssetsData/*/*.bin*

# Visual Scripting auto-generated files
source/GhostGame/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db
source/GhostGame/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db.meta
source/GhostGame/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers
source/GhostGame/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers.meta

# Auto-generated scenes by play mode tests
source/GhostGame/[Aa]ssets/[Ii]nit[Tt]est[Ss]cene*.unity*
